<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Suite1" verbose="1" thread-count="3" parallel="tests">
    <test name="Chrome" preserve-order="true" parallel="methods">
        <parameter name="browser" value="CHROME"/>
        <classes>
            <class name="org.selenium.pom.tests.FormTest"/>
        </classes>
    </test>
    <test name="Firefox" preserve-order="true" parallel="methods">
        <parameter name="browser" value="FIREFOX"/>
        <classes>
            <class name="org.selenium.pom.tests.FormTest"/>
        </classes>
    </test>

</suite>
